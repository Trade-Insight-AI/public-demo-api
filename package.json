{
  "name": "tia-public-demo-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "check": "pnpm tsc --noEmit",
    "build": "pnpm run build:clean:dist && cross-env IGNORE_MIGRATIONS=true nest build",
    "build:clean:dist": "rm -rf dist/*",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:unit": "jest --testRegex='.*\\.spec\\.ts$' --testPathIgnorePatterns='.*\\.spec\\.e2e\\.ts$'",
    "test:e2e": "jest --testRegex='.*\\.spec\\.e2e\\.ts$'",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "migration:create": "cross-env IGNORE_MIGRATIONS=false node create-migration.js",
    "migration:run": "cross-env IGNORE_MIGRATIONS=false ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/@database/data-source-only-for-migration.ts migration:run",
    "migration:revert": "cross-env IGNORE_MIGRATIONS=false ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/@database/data-source-only-for-migration.ts migration:revert"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.842.0",
    "@aws-sdk/s3-request-presigner": "^3.842.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@simplewebauthn/server": "^13.1.2",
    "@types/uuid": "^10.0.0",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "helmet": "^8.1.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.15.6",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "typeorm": "^0.3.22",
    "uuid": "^11.1.0",
    "zod": "^3.24.3",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@jest/globals": "^30.0.4",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcryptjs": "^3.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/multer": "^2.0.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import-helpers": "^2.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.(e2e\\.)?ts$",
    "testPathIgnorePatterns": [
      "<rootDir>/@shared/tests/factories/",
      "<rootDir>/@shared/tests/stubs/",
      "<rootDir>/@shared/tests/setup.ts"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/@shared/tests/setup.ts"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@scarf/scarf",
      "@swc/core",
      "protobufjs"
    ]
  }
}
